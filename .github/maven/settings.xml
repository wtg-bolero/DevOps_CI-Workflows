    <pluginGroups>
        <pluginGroup>org.sonarsource.scanner.maven</pluginGroup>
    </pluginGroups>

    <profiles>
        <profile>
            <id>repository2</id>
            <properties>
                <dependency.resolutionConcurrency>5</dependency.resolutionConcurrency>
                <altSnapshotDeploymentRepository>bolero-snapshots::default::https://${env.REPO_HOSTNAME}/artifactory/bolero-snapshots/</altSnapshotDeploymentRepository>
                <altReleaseDeploymentRepository>bolero-releases::default::https://${env.REPO_HOSTNAME}/artifactory/bolero-releases/</altReleaseDeploymentRepository>
            </properties>
            <repositories>
<!--                <repository>-->
<!--                    <id>central</id>-->
<!--                    <name>Maven Central</name>-->
<!--                    <url>https://repo.maven.apache.org/maven2</url>-->
<!--                    <releases>-->
<!--                        <enabled>true</enabled>-->
<!--                    </releases>-->
<!--                    <snapshots>-->
<!--                        <enabled>false</enabled>-->
<!--                    </snapshots>-->
<!--                </repository>-->
                <repository>
                    <id>repository2</id>
                    <url>https://${env.REPO_HOSTNAME}/artifactory/mirrors-consolidated/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>bolero-releases</id>
                    <url>https://${env.REPO_HOSTNAME}/artifactory/bolero-releases/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>bolero-snapshots</id>
                    <url>https://${env.REPO_HOSTNAME}/artifactory/bolero-snapshots/</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
<!--                <pluginRepository>-->
<!--                    <id>central</id>-->
<!--                    <url>https://repo.maven.apache.org/maven2</url>-->
