                <pluginRepository>
                    <id>repository2</id>
                    <url>https://${env.REPO_HOSTNAME}/artifactory/mirrors-consolidated/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>repository2</activeProfile>
    </activeProfiles>
</settings>
